      <wadl:method id="archive-addArchiveDependency" name="POST">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Record an archive dependency record for the context archive.</p>
<table class="rst-docutils field-list" frame="void" rules="none">
<col class="field-name" />
<col class="field-body" />
<tbody valign="top">
<tr class="rst-field"><th class="rst-field-name" colspan="2">param dependency:</th></tr>
<tr class="rst-field"><td>&nbsp;</td><td class="rst-field-body">is an IArchive object.</td>
</tr>
<tr class="rst-field"><th class="rst-field-name">param pocket:</th><td class="rst-field-body">is an PackagePublishingPocket enum.</td>
</tr>
<tr class="rst-field"><th class="rst-field-name" colspan="2">param component:</th></tr>
<tr class="rst-field"><td>&nbsp;</td><td class="rst-field-body">is the name of a component.  If not given,
the archive dependency will be tied to the component used
for a corresponding source in primary archive.</td>
</tr>
<tr class="rst-field"><th class="rst-field-name">raise:</th><td class="rst-field-body">ArchiveDependencyError if given 'dependency' does not fit
the context archive.</td>
</tr>
<tr class="rst-field"><th class="rst-field-name">return:</th><td class="rst-field-body">a IArchiveDependency object targeted to the context
IArchive requiring 'dependency' IArchive.</td>
</tr>
</tbody>
</table>

</wadl:doc>
        <wadl:request>
          <wadl:representation mediaType="application/x-www-form-urlencoded">
            <wadl:param style="query" name="ws.op" required="true" fixed="addArchiveDependency"/>
            <wadl:param style="query" name="dependency" required="true">
              
              <wadl:link resource_type="https://api.launchpad.net/devel/#archive"/>
            </wadl:param>
            <wadl:param style="query" name="pocket" required="true">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Pocket</p>
<p>The pocket into which this entry is published</p>

</wadl:doc>
              
              <wadl:option value="Release"/>
              <wadl:option value="Security"/>
              <wadl:option value="Updates"/>
              <wadl:option value="Proposed"/>
              <wadl:option value="Backports"/>
            </wadl:param>
            <wadl:param style="query" name="component" required="false">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Component
</wadl:doc>
              
            </wadl:param>
          </wadl:representation>
        </wadl:request>
        <wadl:response>
          <wadl:param name="Location" style="header">
            <wadl:link resource_type="https://api.launchpad.net/devel/#archive_dependency"/>
