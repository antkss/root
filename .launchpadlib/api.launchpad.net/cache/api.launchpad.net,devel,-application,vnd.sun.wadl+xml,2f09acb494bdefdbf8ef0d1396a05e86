              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Include inactive
</wadl:doc>
              
            </wadl:param>
          
        </wadl:request>
        
      </wadl:method>
      <wadl:method id="project-findReferencedOOPS" name="GET">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Find OOPS reports between start_date and end_date.</p>
<table class="rst-docutils field-list" frame="void" rules="none">
<col class="field-name" />
<col class="field-body" />
<tbody valign="top">
<tr class="rst-field"><th class="rst-field-name" colspan="2">param start_date:</th></tr>
<tr class="rst-field"><td>&nbsp;</td><td class="rst-field-body">Do not look in objects whose last modification time
is before this date.</td>
</tr>
<tr class="rst-field"><th class="rst-field-name">param end_date:</th><td class="rst-field-body">Do not look in objects whose last modification time
is after this date.</td>
</tr>
<tr class="rst-field"><th class="rst-field-name">return:</th><td class="rst-field-body">A set of OOPS id's - strings of the form 'OOPS-w+'.</td>
</tr>
</tbody>
</table>

</wadl:doc>
        <wadl:request>
          
            <wadl:param style="query" name="ws.op" required="true" fixed="findReferencedOOPS"/>
            <wadl:param style="query" name="start_date" required="true" type="xsd:dateTime">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Modified after date
</wadl:doc>
              
            </wadl:param>
            <wadl:param style="query" name="end_date" required="true" type="xsd:dateTime">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Modified before date
</wadl:doc>
              
            </wadl:param>
          
        </wadl:request>
        
      </wadl:method>
