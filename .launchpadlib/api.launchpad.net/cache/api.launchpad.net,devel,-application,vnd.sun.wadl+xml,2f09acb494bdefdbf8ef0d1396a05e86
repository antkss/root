        <wadl:link resource_type="https://api.launchpad.net/devel/#translation_import_queue_entry"/>
      </wadl:param>
  </wadl:representation>

  
  
    <wadl:resource_type id="vulnerability">
      <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
