      <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Interface for ArchiveSet
</wadl:doc>
      <wadl:method name="GET" id="archives-get">
        <wadl:response>
          <wadl:representation href="https://api.launchpad.net/devel/#archive-page"/>
          <wadl:representation mediaType="application/vnd.sun.wadl+xml" id="archives-wadl"/>
        </wadl:response>
      </wadl:method>

      <wadl:method id="archives-getByReference" name="GET">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Return the IArchive with the given archive reference.
</wadl:doc>
        <wadl:request>
          
            <wadl:param style="query" name="ws.op" required="true" fixed="getByReference">
              <wadl:doc>The name of the operation being invoked.</wadl:doc>
            </wadl:param>
            <wadl:param style="query" name="reference" required="true">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Archive reference string
</wadl:doc>
              
            </wadl:param>
          
        </wadl:request>
        <wadl:response>
          
