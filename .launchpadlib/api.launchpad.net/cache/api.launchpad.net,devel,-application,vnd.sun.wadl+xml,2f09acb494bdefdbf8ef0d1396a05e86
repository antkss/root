</wadl:doc>
        <wadl:link resource_type="https://api.launchpad.net/devel/#source_package_recipe_build"/>
      </wadl:param>
      <wadl:param style="plain" required="true" name="name" path="$['name']">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Name</p>
