</wadl:doc>
        <wadl:link resource_type="https://api.launchpad.net/devel/#person"/>
      </wadl:param>
      <wadl:param style="plain" required="true" name="bug_link" path="$['bug_link']">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Bug
</wadl:doc>
        <wadl:link resource_type="https://api.launchpad.net/devel/#bug"/>
      </wadl:param>
