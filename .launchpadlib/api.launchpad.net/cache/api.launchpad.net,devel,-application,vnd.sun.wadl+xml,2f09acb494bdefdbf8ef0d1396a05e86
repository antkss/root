             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Specification URL</p>
<p>The URL of the specification. This is usually a wiki page.</p>

</wadl:doc>
              
            </wadl:param>
            <wadl:param style="query" name="summary" required="true">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Summary</p>
<p>A single-paragraph description of the feature. This will also be displayed in most feature listings.</p>

</wadl:doc>
              
            </wadl:param>
            <wadl:param style="query" name="definition_status" required="false">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Definition Status</p>
<p>The current status of the process to define the feature and get approval for the implementation plan.</p>

</wadl:doc>
              
              <wadl:option value="Approved"/>
              <wadl:option value="Pending Approval"/>
              <wadl:option value="Review"/>
              <wadl:option value="Drafting"/>
              <wadl:option value="Discussion"/>
              <wadl:option value="New"/>
              <wadl:option value="Superseded"/>
              <wadl:option value="Obsolete"/>
            </wadl:param>
            <wadl:param style="query" name="assignee" required="false">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Assignee</p>
<p>The person responsible for implementing the feature.</p>

</wadl:doc>
              <wadl:link resource_type="https://api.launchpad.net/devel/#person"/>
            </wadl:param>
            <wadl:param style="query" name="drafter" required="false">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Drafter</p>
<p>The person responsible for drafting the specification.</p>

</wadl:doc>
              <wadl:link resource_type="https://api.launchpad.net/devel/#person"/>
            </wadl:param>
            <wadl:param style="query" name="whiteboard" required="false">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Status Whiteboard</p>
<p>Any notes on the status of this spec you would like to make. Your changes will override the current text.</p>

</wadl:doc>
              
            </wadl:param>
            <wadl:param style="query" name="target" required="true">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
The product or distribution context of this specification.
</wadl:doc>
              <wadl:link resource_type="https://api.launchpad.net/devel/#specification_target"/>
            </wadl:param>
          </wadl:representation>
        </wadl:request>
        <wadl:response>
          <wadl:param name="Location" style="header">
            <wadl:link resource_type="https://api.launchpad.net/devel/#specification"/>
          </wadl:param>
          
        </wadl:response>
      </wadl:method>
    </wadl:resource_type>

  
  
