      <wadl:param style="plain" required="true" name="all_specifications_collection_link" path="$['all_specifications_collection_link']">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>All specifications</p>
<p>A list of all specifications, regardless of status or approval or completion, for this object.</p>

</wadl:doc>
        <wadl:link resource_type="https://api.launchpad.net/devel/#specification-page-resource"/>
      </wadl:param>
