      </wadl:method>

      <wadl:method name="PUT" id="object_with_translation_imports-put">
        <wadl:request>
          <wadl:representation href="https://api.launchpad.net/devel/#object_with_translation_imports-full"/>
        </wadl:request>
      </wadl:method>

