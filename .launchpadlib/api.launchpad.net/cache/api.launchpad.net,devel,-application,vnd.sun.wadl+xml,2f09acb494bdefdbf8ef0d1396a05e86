</wadl:doc>
        <wadl:link resource_type="https://api.launchpad.net/devel/#processor-page-resource"/>
      </wadl:param>
      <wadl:param style="plain" required="true" name="registrant_link" path="$['registrant_link']">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
<p>Registrant</p>
<p>The person who registered this base.</p>

</wadl:doc>
        <wadl:link resource_type="https://api.launchpad.net/devel/#person"/>
      </wadl:param>
    </wadl:representation>

