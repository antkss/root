</tr>
</tbody>
</table>

</wadl:doc>
        <wadl:request>
          <wadl:representation mediaType="application/x-www-form-urlencoded">
            <wadl:param style="query" name="ws.op" required="true" fixed="updatePillarSharingPolicies"/>
            <wadl:param style="query" name="pillar" required="true">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Pillar
</wadl:doc>
              <wadl:link resource_type="https://api.launchpad.net/devel/#pillar"/>
            </wadl:param>
            <wadl:param style="query" name="branch_sharing_policy" required="false">
              
              
              <wadl:option value="Public"/>
              <wadl:option value="Public, can be proprietary"/>
              <wadl:option value="Proprietary, can be public"/>
              <wadl:option value="Proprietary"/>
              <wadl:option value="Embargoed, can be proprietary"/>
              <wadl:option value="Forbidden"/>
            </wadl:param>
            <wadl:param style="query" name="bug_sharing_policy" required="false">
              
              
              <wadl:option value="Public"/>
              <wadl:option value="Public, can be proprietary"/>
              <wadl:option value="Proprietary, can be public"/>
              <wadl:option value="Proprietary"/>
              <wadl:option value="Forbidden"/>
              <wadl:option value="Embargoed, can be proprietary"/>
            </wadl:param>
            <wadl:param style="query" name="specification_sharing_policy" required="false">
              
              
              <wadl:option value="Public"/>
              <wadl:option value="Public, can be proprietary"/>
              <wadl:option value="Proprietary, can be public"/>
              <wadl:option value="Proprietary"/>
              <wadl:option value="Embargoed, can be proprietary"/>
              <wadl:option value="Forbidden"/>
            </wadl:param>
          </wadl:representation>
        </wadl:request>
        
      </wadl:method>
    </wadl:resource_type>


