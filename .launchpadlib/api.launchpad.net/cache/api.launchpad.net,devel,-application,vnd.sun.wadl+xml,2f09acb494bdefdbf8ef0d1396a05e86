              <wadl:link resource_type="https://api.launchpad.net/devel/#HostedFile"/>
            </wadl:param>
            <wadl:param style="query" name="comment" required="true">
              
              
            </wadl:param>
            <wadl:param style="query" name="filename" required="false">
              
              
            </wadl:param>
            <wadl:param style="query" name="url" required="false">
              
              
            </wadl:param>
            <wadl:param style="query" name="is_patch" required="false">
              
              
            </wadl:param>
            <wadl:param style="query" name="content_type" required="false">
              
              
            </wadl:param>
            <wadl:param style="query" name="description" required="false">
              
              
            </wadl:param>
          </wadl:representation>
        </wadl:request>
        <wadl:response>
          <wadl:param name="Location" style="header">
            <wadl:link resource_type="https://api.launchpad.net/devel/#bug_attachment"/>
