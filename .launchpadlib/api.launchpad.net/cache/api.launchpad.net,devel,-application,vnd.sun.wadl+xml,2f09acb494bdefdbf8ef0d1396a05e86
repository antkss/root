      <wadl:method id="distribution-newVulnerability" name="POST">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Create a new vulnerability in the distribution.
</wadl:doc>
        <wadl:request>
          <wadl:representation mediaType="application/x-www-form-urlencoded">
            <wadl:param style="query" name="ws.op" required="true" fixed="newVulnerability"/>
            <wadl:param style="query" name="status" required="true">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
The status of the vulnerability.
</wadl:doc>
              
              <wadl:option value="Needs triage"/>
              <wadl:option value="Active"/>
              <wadl:option value="Ignored"/>
              <wadl:option value="Retired"/>
            </wadl:param>
            <wadl:param style="query" name="creator" required="true">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Person creating the vulnerability.
</wadl:doc>
              <wadl:link resource_type="https://api.launchpad.net/devel/#person"/>
            </wadl:param>
            <wadl:param style="query" name="information_type" required="true">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Information Type. Defaults to Public.
</wadl:doc>
              
              <wadl:option value="Public"/>
              <wadl:option value="Public Security"/>
              <wadl:option value="Private Security"/>
              <wadl:option value="Private"/>
              <wadl:option value="Proprietary"/>
              <wadl:option value="Embargoed"/>
            </wadl:param>
            <wadl:param style="query" name="importance" required="false">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Indicates the work priority, not the severity. Defaults to Undecided.
</wadl:doc>
              
              <wadl:option value="Unknown"/>
              <wadl:option value="Undecided"/>
              <wadl:option value="Critical"/>
              <wadl:option value="High"/>
              <wadl:option value="Medium"/>
              <wadl:option value="Low"/>
              <wadl:option value="Wishlist"/>
            </wadl:param>
            <wadl:param style="query" name="cve" required="false">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
External CVE reference corresponding to this vulnerability, if any.
</wadl:doc>
              <wadl:link resource_type="https://api.launchpad.net/devel/#cve"/>
            </wadl:param>
            <wadl:param style="query" name="description" required="false">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
A short description of the vulnerability.
</wadl:doc>
              
            </wadl:param>
            <wadl:param style="query" name="notes" required="false">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Free-form notes for this vulnerability.
</wadl:doc>
              
            </wadl:param>
            <wadl:param style="query" name="mitigation" required="false">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Explains why we're ignoring this vulnerability.
</wadl:doc>
              
            </wadl:param>
            <wadl:param style="query" name="importance_explanation" required="false">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Used to explain why our importance differs from somebody else's CVSS score.
</wadl:doc>
              
            </wadl:param>
            <wadl:param style="query" name="date_made_public" required="false" type="xsd:dateTime">
              <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
The date this vulnerability was made public.
</wadl:doc>
              
            </wadl:param>
          </wadl:representation>
        </wadl:request>
        
      </wadl:method>
