</wadl:doc>
        <wadl:link resource_type="https://api.launchpad.net/devel/#branch_subscription-page-resource"/>
      </wadl:param>
      <wadl:param style="plain" required="true" name="unique_name" path="$['unique_name']">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
