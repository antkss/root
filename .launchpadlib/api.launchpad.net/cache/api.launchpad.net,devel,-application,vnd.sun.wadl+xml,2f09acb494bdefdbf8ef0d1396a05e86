</wadl:doc>
              
              <wadl:option value="Open Team"/>
              <wadl:option value="Delegated Team"/>
              <wadl:option value="Moderated Team"/>
              <wadl:option value="Restricted Team"/>
            </wadl:param>
          </wadl:representation>
        </wadl:request>
