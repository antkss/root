            </wadl:param>
          
        </wadl:request>
        <wadl:response>
          
          <wadl:representation href="https://api.launchpad.net/devel/#snap-page"/>
        </wadl:response>
      </wadl:method>
      <wadl:method id="snaps-new" name="POST">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
