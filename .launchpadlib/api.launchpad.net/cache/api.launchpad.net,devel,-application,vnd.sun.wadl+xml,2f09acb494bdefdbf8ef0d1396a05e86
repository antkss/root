<p>The policy this project or distribution uses to  balance openness and control for their translations.</p>

</wadl:doc>
        
        <wadl:option value="Open"/>
        <wadl:option value="Structured"/>
        <wadl:option value="Restricted"/>
        <wadl:option value="Closed"/>
      </wadl:param>
      <wadl:param style="plain" required="true" name="translations_usage" path="$['translations_usage']">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
